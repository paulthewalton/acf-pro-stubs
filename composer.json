{
    "name": "paulthewalton/acf-pro-stubs",
    "type": "library",
    "description": "Advanced Custom Fields Pro function, class and global variable declaration stubs for easier static analysis.",
    "keywords": [
        "wordpress",
        "static analysis",
        "acf",
        "advanced custom fields",
        "pro"
    ],
    "require": {
        "php-stubs/wordpress-stubs": "^4.7 || ^5.0"
    },
    "require-dev": {
        "php": "^7.1",
        "ext-gettext": "*",
        "giacocorsiglia/stubs-generator": "^0.5.0",
        "advanced-custom-fields/advanced-custom-fields-pro": "*"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Paul Walton",
            "email": "pwalton@live.ca",
            "role": "copycat"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.8.9",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%ACF_PRO_KEY}&t={%VERSION}"
                },
                "require": {
                    "composer/installers": "^1.4",
                    "ffraenz/private-composer-installer": "^4.0"
                }
            }
        }
    ]
